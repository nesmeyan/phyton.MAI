   ],
   "source": [
    "def zig(a, b):\n",
    "    if len(a) > len(b):\n",
    "        length = len(b)\n",
    "    else:\n",
    "        length = len(a)\n",
    "\n",
    "    for i in range(0, length):\n",
    "        d = (a[i], b[i])\n",
    "        yield d\n",
    "\n",
    "\n",
    "print(list(zig('MAI', 'phyton')))\n",
    "print(list(zig('qq', [3, 1, 0, 3])))"
   ]
