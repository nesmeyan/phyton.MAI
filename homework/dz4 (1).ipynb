   ],
   "source": [
    "def ky(arr):\n",
    "    num = 0\n",
    "    for i in arr:\n",
    "        b = (num, i)\n",
    "        yield b\n",
    "        num += 1\n",
    "\n",
    "\n",
    "print(list(ky(['MAI', 'qq', 'phyton'])))\n",
    "print(list(ky('qq MAI!')))"
   ]
